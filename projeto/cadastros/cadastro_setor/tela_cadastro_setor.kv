<Tela_Cadastro_Setor>:
    
    BoxLayout:
        orientation: 'vertical'
        spacing: 10
        padding: 10

        Button:
            text: 'Carregar setores'
            size_hint_y: None
            on_press: root.atualizar_dados()
            height: '40dp'

        ScrollView:
            GridLayout:
                id: container
                cols: 1
                size_hint_y: None
                height: self.minimum_height
                row_default_height: '40dp'
                row_force_default: True

        Button:
            text: 'Ir para cadastro de setor'
            size_hint_y: None
            height: "40dp"
            on_press: app.root.current = "cadastro_setor"
        
        Button:
            text: 'Voltar'
            size_hint_y: None
            height: "40dp"
            on_press: root.manager.current = "Menu_cadastros"